	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@zyrohub/config-prettier": "^1.0.6",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
