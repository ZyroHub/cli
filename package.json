{
	"name": "@zyrohub/cli",
	"version": "1.0.0",
	"author": "DMVMarcio",
	"type": "module",
	"description": "A CLI Utility for the ZyroHub Ecosystem",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
